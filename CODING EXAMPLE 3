package selfProgramming;

public class Example3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int x=10;
		double y=5.373123;
		
		System.out.printf("Value of x is %d and the value of y is %6.2f%n",x,y);
		System.out.printf("%-10s","Total:");
		System.out.printf("%n%10.2f",y);
		System.out.printf("%n%-10s%10.2f","Total:",y);
	}

}
